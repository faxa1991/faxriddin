	<script src="assets/vendor/jquery/jquery.js"></script>
	
	
	<script>

	 $(document).ajaxSend(function(e, xhr, opt){  
		 xhr.setRequestHeader('Authorization', localStorage.token);
		      
	 });
	
	$.ajax({
		url:"${backend_url}/api/reports/v1/shift/data",
		method:"POST",
		contentType: "application/json",
		data:JSON.stringify({"start":0,"length":10000}),
		async:false,
		success:function(data){
			
			console.log(data);
			for(var i=0;i<data.data.length;i++){
				var clone = $('#nav_employes').find('ul').find('li').first().clone();
				clone.find('a').html("Сотрудники / "+data.data[i].shift.name);
				clone.attr('id','nav_employes_'+data.data[i].id);
				clone.find('a').attr('href','/employes-shift?shift='+data.data[i].id+'&name='+data.data[i].shift.name);
				$('#nav_employes').find('ul').append(clone);
				
				
			}
			
			
		}

	});
	
	
	

	
	 
	 
	 $(".nav-pagex").unbind('click');
	 $(".nav-pagex").on('click',function(){
	   
		 
		 localStorage.setItem('li1',$(this).attr('id'));
		 localStorage.setItem('li2',$(this).closest('ul').closest('li').attr('id'));
	   
		 
	 });
	
	 if(localStorage.getItem('li1')!=null){
	 $('#'+localStorage.getItem('li1')).addClass('nav-active');
	 $('#'+localStorage.getItem('li2')).addClass('nav-expanded');
	 }

	

	
	
	
	</script>				